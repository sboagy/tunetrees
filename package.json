    "sql.js": "^1.13.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tailwindcss-animate": "^1.0.7",
    "ts-fsrs": "^5.2.3",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@playwright/test": "^1.56.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/better-sqlite3": "^7.6.13",
