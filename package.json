    "better-sqlite3": "^12.4.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.5",
    "postgres": "^3.4.7",
    "shadcn-solid": "^0.7.7",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.0.3",
