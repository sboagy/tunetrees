    "@tanstack/solid-table": "^8.21.3",
    "@tanstack/solid-virtual": "^3.13.12",
    "abcjs": "^6.5.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.6",
    "jodit": "^4.6.13",
    "jodit-react": "^5.2.26",
    "loglevel": "^1.9.2",
    "lucide-solid": "^0.545.0",
