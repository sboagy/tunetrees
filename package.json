{
  "name": "tunetrees",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "migrate:supabase": "tsx scripts/migrate-to-supabase.ts",
    "migrate:production": "tsx scripts/migrate-production-to-supabase.ts",
    "create:views": "tsx scripts/create-views-direct.ts",
    "lint": "biome lint",
    "format": "biome format",
    "check": "biome check",
    "fix": "biome check --write",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "test:unit": "vitest run tests/",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright test --reporter=html",
    "playwright:install": "playwright install"
