{
  "compilerOptions": {
    //    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    // "sourceMap": true,
    "outDir": ".next",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "inlineSourceMap": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "auth": [
        "./auth/"
      ],
      // "next-auth-http-adapter": [
      //   "./../../../gitmisc/next-auth-http-adapter/src"
      // ]
    },
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/**/*.tsx",
    "/Users/sboag/gitmisc/next-auth/**/*.ts",
    "/Users/sboag/gitmisc/next-auth/**/*.tsx",
    // "/Users/sboag/gitmisc/next-auth-http-adapter/**/*.ts",
    // "/Users/sboag/gitmisc/next-auth-http-adapter/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}